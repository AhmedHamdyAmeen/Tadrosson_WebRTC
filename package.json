{
    "name": "Tadrosoon",
    "version": "1.0.1",
    "description": "A free WebRTC browser-based video call",
    "main": "server.js",
    "scripts": {
        "dev": "nodemon app/src/server.js",
        "start": "node app/src/server.js",
        "test": "echo \"Error: no test specified\" && exit 1",
        "lint": "npx prettier --write .",
        "migration:run": "db-migrate up",
